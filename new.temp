#include <bits/stdc++.h>\n\nusing namespace std;\nvector<pair<int, int>> adj[1000001];\nint main()\n{\n    int n;\n    cin >> n;\n    unordered_map<int, int> mp;\n    for (size_t i = 0; i < n; ++i)\n    {\n        int x;\n        cin >> x;\n        mp[x] = INT_MAX;\n    }\n    int e;\n    cin >> e;\n    for (size_t i = 0; i < e; ++i)\n    {\n        int x, y, d;\n        cin >> x, y, d;\n        adj[x].push_back({y, d});\n    }\n    int src, dest;\n    cin >> src, dest;\n    set<pair<int, int>> s;\n    mp[src] = 0;\n    s.insert({0, src});\n    while (!s.empty())\n    {\n        pair<int, int> tmp = *(s.begin());\n        s.erase(s.begin());\n        int u = tmp.second;\n        for (auto i = adj[u].begin(); i != adj[u].end(); ++i)\n        {\n            int v = (*i).second;\n            int weight = (*i).second;\n            if (m[v] > mp[u] + weight)\n            {\n                if (mp[v] != INT_MAX)\n                {\n                    s.erase(s.find({mp[v], v}));\n                }\n                mp[v] = mp[u] + weight;\n                s.insert({mp[v], v});\n            }\n        }\n    }\n\n    if (mp[dest] == INT_MAX)\n        cout << "-1" << endl;\n    else\n        cout << mp[dest] << endl;\n\n    return 0;\n}\n\n\n